{
  "name": "apptaxes",
  "version": "1.0.0",
  "description": "GraphQL backend for a personalized expense tracker application",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "pretty": "npx prettier --write .",
    "knex": "npx knex --knexfile src/database/knex/knexfile.ts",
    "rollback-all": "npx knex migrate:rollback --all --knexfile src/database/knex/knexfile.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "postinstall": "npm run build",
    "prod-rollback-all": "knex migrate:rollback --all --knexfile dist/database/knex/knexfile.js",
    "knex-prod": "knex --knexfile dist/database/knex/knexfile.js",
    "prod-migrations": "npm run prod-rollback-all && npm run knex-prod migrate:latest && npm run knex-prod seed:run"
  },
  "keywords": [
    "graphql",
    "typescript",
    "nodejs",
    "express"
  ],
  "author": "John Eric Siguenza",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.6.1",
    "inversify": "^6.0.1",
    "knex": "^0.95.13",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.4",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@types/shelljs": "^0.8.9",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
